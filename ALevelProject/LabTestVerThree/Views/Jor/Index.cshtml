@model IEnumerable<LabTestVerThree.Models.EventsDetails>

@{
    ViewBag.Title = "События";
}

<div class="row">
    <div class="col-md-12">
        <div class="panel panel-primary list-panel" id="list-panel">
            <div class="panel-heading list-panel-heading">
                <h1 class="panel-title list-panel-title">Заказы</h1>
            </div>
            <div class="panel-body">
                <table id="assets-data-table" class="table table-striped table-bordered" style="width:100%">
                    <thead>
                        <tr>
                            <th>Date</th>
                            <th>EventMessage</th>
                            <th>ActionName</th>
                            <th>ControllerName</th>
                            <th>StackTrace</th>
                        </tr>
                    </thead>
                    <tbody>
                        @foreach (var asset in Model)
                        {
                            <tr>
                                <td>@asset.Date</td>
                                <td>@asset.EventMessage</td>
                                <td>@asset.ActionName</td>
                                <td>@asset.ControllerName</td>
                                <td>@asset.StackTrace</td>

                            </tr>
                        }
                    </tbody>
                </table>
            </div>
        </div>
    </div>
</div>
<div>
    @Html.ActionLink("Back to List", "Index", "Home")
</div>
@section Scripts
{

    <script type="text/javascript">
        $(document).ready(function () {

            $('#assets-data-table').DataTable();
        });
    </script>

}